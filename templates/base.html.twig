    <!DOCTYPE html>
    <html lang="fr">
        <head>
            <meta charset="UTF-8">
            <title>{% block title %}Page de base{% endblock %}</title>
            <link rel="stylesheet" href="{{ asset('../node_modules/bulma/css/bulma.css') }}">
            <link rel="stylesheet" href="{{ asset('../node_modules/bulma/css/bulma-rtl.css') }}">
            <link rel="stylesheet" href="{{ asset('../node_modules/bulma/bulma.sass') }}">

            {%- block stylesheets -%}
                <link rel="stylesheet" href="{{ absolute_url(asset('build/bulmaStyles.scss')) }}"/>
                {{ encore_entry_link_tags('Bulma') }}
