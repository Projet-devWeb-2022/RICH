                {{ encore_entry_link_tags('app') }}
            {% endblock %}#}

            {% block javascripts %}
                {{ encore_entry_script_tags('app') }}
            {% endblock %}
        </head>
        <body>
            {% block nav %}
                <nav class="navbar is-primary" role="navigation" aria-label="main navigation">
                    <div class="navbar-brand">
                            <a class="navbar-item">
                                <img src="{{ asset('../assets/img/logo_RICH.png') }}">
                            </a>
                    </div>
                    <div class = "navbar-menu">
                        <div class="navbar-start">
                            <a class="navbar-item">
                                test
                            </a>
                        </div>
                        <div class="navbar-end">
                            <a class="navbar-item">
                                <img src="{{ asset('../assets/img/logo_RICH.png') }}">
                            </a>
                        </div>
                    </div>
                </nav>

                <nav style="background-color: #FDEE00; height: 0.8vh"></nav>
